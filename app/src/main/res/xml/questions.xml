<?xml version="1.0" encoding="utf-8"?>
<Inspection>
    <Equipment type="Tank">
        <Section name="General">
            <Question
                name="manway_available"
                text="Manway available"
                type="bool" />
            <Question
                name="manway_type"
                text="Manway type:"
                type="choice">
                <Answer answer="Scaffold" />
                <Answer answer="Manlift - Drivable" />
                <Answer answer="Manlift - towable" />
            </Question>
            <Question
                name="additional_notes"
                text="Additional notes"
                dependsOn="manway_available:Yes|maybe"
                type="input_text" />
            <Question
                name="manway_photos"
                text="Manway Photos"
                dependsOn="manway_available:Yes"
                type="photo" />
            <Question
                name="operating_temperature"
                text="Operating Temperature"
                type="input_numeric" />
        </Section>
        <Section name="Insulation">
        </Section>
    </Equipment>
    <Equipment type="Vessel">
        <Section name="Data Plate">
            <Question>
            </Question>
        </Section>
    </Equipment>
    <Equipment type="Air Receiver">
        <Section name="section">
        </Section>
    </Equipment>
</Inspection>